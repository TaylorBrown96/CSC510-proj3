# Windows Console Encoding Fix
PYTHONIOENCODING=utf-8

# Database Configuration
DATABASE_URL=sqlite:///./proj2.db
DATABASE_NAME=proj2.db

# Environment
ENVIRONMENT=development

# JWT Configuration
JWT_SECRET_KEY=your-secret-key-here-change-in-production-min-32-chars
JWT_ALGORITHM=HS256
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=30

GEMINI_API_KEY=test

# Frontend URL
FRONTEND_URL=http://localhost:5173

# Default LLM model used by the backend RecommendationService when
# clients don't specify a model. Use a Gemini model by default (local
# environment doesn't include Anthropic/Claude models).
GEMINI_MODEL=gemini-2.5-flash

# Google Maps API Key
GOOGLE_MAPS_API_KEY=your-google-maps-api-key-here
